<p class="alert alert-success"><%= notice %></p>
<h1>Usuários</h1>
<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Ver este usuário", user, class: "btn btn-primary" %>
    </p>
  <% end %>
</div>
<%= link_to "Novo usuário", new_user_path, class: "btn btn-success" %>